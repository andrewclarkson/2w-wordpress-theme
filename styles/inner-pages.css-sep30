/* =RedeemBar Script
-------------------------------------------------------------- */

element.style {
top:0;
}
#redeembar {
background:none repeat scroll 0 0 #da1c1e;
color:#ffffff;
font-family:Arial, Helvetica, sans-serif;
font-size:15px;
font-weight:normal;
height:42px;
line-height:43px;
margin:0;
overflow:visible;
padding:0;
position:relative;
text-align:center;
width:100%;
z-index:1000;
}
#redeembar a {
color:#ffffff;
text-decoration:underline;
}
#redeembar a:hover {
color:#ffffff;
text-decoration:none;
}